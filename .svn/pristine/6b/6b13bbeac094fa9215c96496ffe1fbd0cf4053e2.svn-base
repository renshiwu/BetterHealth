
body,
section,
article,
aside,
ul,
ol,
li,
p,
h1,
h2,
h3,
h4,
h5,
h6,
img,
textarea,
input,
select,
form {
    margin: 0;
    padding: 0;
}

body,
input,
textarea,
select,
h1,
h2,
h3,
h4,
h5,
h6 {
    font: normal 12px/1 Helvetica Neue, Microsoft yahei, arial;
    color: #000;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

table {
    border-collapse: collapse;
    border-spacing: 0;
}

a {
    color: #000;
    text-decoration: none;
}

em,
i {
    font-style: normal;
}

ul,
ol {
    list-style: none;
}

img {
    max-width: 100%;
    border: none;
    vertical-align: middle;
}

input,
button {
    -webkit-appearance: none;
    outline: none;
}

p {
    word-break: break-word;
}

.c:after {
    content: "";
    display: table;
    clear: both;
}

.clamp {
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    display: -webkit-box;
    -webkit-box-orient: vertical;
}

.hor,
.ell,
.arrow {
    display: inline-block;
    vertical-align: middle;
}

.arrow {
    width: 0;
    height: 0;
    overflow: hidden;
    border: 6px solid transparent;
}

.ell {
    max-width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.hide {
    display: none;
}

.fl {
    float: left;
}

.fr {
    float: right;
}


